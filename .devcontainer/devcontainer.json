{
    "name": "Rust, Python, and AWS CLI",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer", // Updated to use rust-analyzer
                "ms-azuretools.vscode-docker", // Docker extension
                "ms-python.python" // Added Python extension
            ]
        }
    },
    "postCreateCommand": "rustup update && rustup component add clippy && rustup component add rustfmt && pip install --upgrade pip"
}
